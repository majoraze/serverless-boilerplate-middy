{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30
    },
    "birthDate": {
      "type": "string",
      "format": "date-time"
		},
		"cpf": {
			"type": "string",
			"pattern": "\\d{11}"
		}
  },
  "additionalProperties": false,
  "minProperties" : 1
}